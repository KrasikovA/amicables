angular.module('app')
.controller('AboutCtrl',[
'$scope',
'$stateParams',
'slides',
'leaf',
'aboutBack',
'aboutDescription',
'$sce',
function($scope, $stateParams, slides,leaf,aboutBack,aboutDescription,$sce){
	var self = this;
	leaf.navigate('about',slides.slidesList);
	self.aboutBack = aboutBack.image_url;
	self.description = $sce.trustAsHtml(aboutDescription.description);
}])